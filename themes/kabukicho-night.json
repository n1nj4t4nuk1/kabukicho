  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#6B5E85",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#FFD37E"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#7CFFBD"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#7AA2FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF7AC6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#FFD37E"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF94D6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FF7AC6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "variable.function"],
      "settings": {
        "foreground": "#7CFFBD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#7AA2FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FFD37E"
      }
    },
    {
      "name": "Markup heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#FF7AC6"
      }
    },
    {
      "name": "Markup link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#7CFFE1"
      }
    }
  ]
}
